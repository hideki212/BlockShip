<div id="content">
    <div id="content-header">
        <div id="breadcrumb"> <a href="/user/" title="Go to Home" class="tip-bottom"><i class="icon-home"></i>
                Home</a> </div>
    </div>
    <div class="container-fluid">
        <div class="row-fluid">
            <div class="span12">
                <div class="widget-box">
                    <div class="widget-title"> <span class="icon"> <i class="icon-align-justify"></i> </span>
                        <h5>Create Bill</h5>
                    </div>
                    <div class="widget-content nopadding">
                        <form method="POST" action="http://localhost:/3005/edituser" class="form-horizontal">
                            <div class="control-group">
                                <label class="control-label">Bill Number :</label>
                                <div class="controls">
                                    <input type="text" class="span11" placeholder="Bill Number" name="BolNumber" />
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label">Carrier :</label>
                                <div class="controls">
                                    <input type="text" class="span11" placeholder="Carrier name" name="CarrierName" />
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label">SPAC :</label>
                                <div class="controls">
                                    <input type="text" class="span11" placeholder="SPAC" name="SPAC" />
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label">Trailer Number :</label>
                                <div class="controls">
                                    <input type="text" class="span11" placeholder="Trailer Number"
                                        name="TrailerNumber" />
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label">Pro Number :</label>
                                <div class="controls">
                                    <input type="text" class="span11" placeholder="Pro Number" name="ProNumber" />
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label">Serial Number :</label>
                                <div class="controls">
                                    <input type="text" class="span11" placeholder="Serial Number" name="SerialNumber" />
                                </div>
                            </div>
                            <label>Ship from: </label>
                            <div class="control-group">
                                <label class="control-label">Name :</label>
                                <div class="controls">
                                    <input type="text" class="span11" placeholder="Name" name="Shipfromname" />
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label">SID Number :</label>
                                <div class="controls">
                                    <input type="text" class="span11" placeholder="SID No" name="SIDNo" />
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label">Address Line 1 :</label>
                                <div class="controls">
                                    <input type="text" class="span11" placeholder="Address Line 1"
                                        name="FromAddressLine1" />
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label">Address Line 1 :</label>
                                <div class="controls">
                                    <input type="text" class="span11" placeholder="Address Line 2"
                                        name="FromAddressLine2" />
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label">City :</label>
                                <div class="controls">
                                    <input type="text" class="span11" placeholder="City" name="FromCity" />
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label">State :</label>
                                <div class="controls">
                                    <input type="text" class="span11" placeholder="State" name="FromState" />
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label">Zip Code :</label>
                                <div class="controls">
                                    <input type="text" class="span11" placeholder="Zip Code" name="FromZipCode" />
                                </div>
                            </div>
                            <label>Ship to:</label>
                            <div class="control-group">
                                <label class="control-label">Name :</label>
                                <div class="controls">
                                    <input type="text" class="span11" placeholder="Name" name="Shiptoname" />
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label">CID Number :</label>
                                <div class="controls">
                                    <input type="text" class="span11" placeholder="CID No" name="CIDNo" />
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label">Address Line 1 :</label>
                                <div class="controls">
                                    <input type="text" class="span11" placeholder="Address Line 1"
                                        name="ToAddressLine1" />
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label">Address Line 2 :</label>
                                <div class="controls">
                                    <input type="text" class="span11" placeholder="Address Line 2"
                                        name="ToAddressLine2" />
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label">City :</label>
                                <div class="controls">
                                    <input type="text" class="span11" placeholder="City" name="ToCity" />
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label">State :</label>
                                <div class="controls">
                                    <input type="text" class="span11" placeholder="State" name="ToState" />
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label">Zipcode :</label>
                                <div class="controls">
                                    <input type="text" class="span11" placeholder="Zip Code" name="ToZipCode" />
                                </div>
                            </div>
                            <label>Third Party Freight charges:</label>
                            <div class="control-group">
                                <label class="control-label">Name :</label>
                                <div class="controls">
                                    <input type="text" class="span11" placeholder="Name" name="Thirdpartyname" />
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label">Address Line 1 :</label>
                                <div class="controls">
                                    <input type="text" class="span11" placeholder="Address Line 1"
                                        name="TPAddressLine1" />
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label">Address Line 2 :</label>
                                <div class="controls">
                                    <input type="text" class="span11" placeholder="Address Line 2"
                                        name="TPAddressLine2" />
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label">City :</label>
                                <div class="controls">
                                    <input type="text" class="span11" placeholder="City" name="TPCity" />
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label">State :</label>
                                <div class="controls">
                                    <input type="text" class="span11" placeholder="State" name="TPState" />
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label">Zipcode :</label>
                                <div class="controls">
                                    <input type="text" class="span11" placeholder="Zip Code" name="TPZipCode" />
                                </div>
                            </div>
                            <label>Freight Charges Terms:</label>

                            <div class="control-group">
                                <label class="control-label">Select input</label>
                                <div class="controls">
                                    <select name="FCT">
                                        <option value="Prepaid">Prepaid</option>
                                        <option value="Collect">Collect</option>
                                        <option value="3rd Party">3rd Party</option>
                                        <option disabled selected>Select One</option>
                                    </select>
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label">FCT Value :</label>
                                <div class="controls">   
                                    <input type="text" class="span11" placeholder="Name" name="fctval" />
                                </div>
                            </div>
                            <div class="control-group">
                                <div class="controls">
                                    <label>
                                        <input class="checkbox" type="checkbox" name="Masterbill" value="Masterbill" checked />
                                        Master bill of lading with attached underlying bills of lading</label>
                                </div>
                            </div>

                            <div class="form-actions">
                                <input class="form-control btn btn-primary" type="button" value="Submit"
                                    onclick="submitBill()">
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>